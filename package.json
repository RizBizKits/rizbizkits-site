{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "npm run build && node ./scripts/deploy-github"
  },
  "dependencies": {
    "gatsby": "^2.14.0",
    "gatsby-plugin-catch-links": "^2.1.5",
    "gatsby-plugin-feed": "^2.3.15",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-matomo": "^0.7.2",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-twitter": "^2.1.19",
    "gatsby-plugin-typography": "^2.3.0",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-responsive-iframe": "^2.2.10",
    "gatsby-remark-social-cards": "^0.4.1",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-remark": "^5.25.1",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "smooth-scroll": "^16.1.0",
    "typeface-playfair-display": "0.0.72",
    "typewriter-effect": "^2.12.2",
    "typography": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19",
    "typography-theme-funston": "^0.16.19",
    "typography-theme-stow-lake": "^0.16.19"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
